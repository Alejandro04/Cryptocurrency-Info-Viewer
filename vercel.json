{
  "version": 2,
  "builds": [
    {
      "src": "app/app.py",
      "use": "@vercel/python"
    },
    {
      "src": "ui/package.json",
      "use": "@vercel/static-build"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "app/app.py"
    },
    {
      "src": "/(.*)",
      "dest": "ui/.svelte-kit/output/client/$1"
    }
  ]
}
